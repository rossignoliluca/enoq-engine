{
  "name": "enoq-core",
  "version": "2.0.0",
  "description": "ENOQ - Sistema Operativo Totale per l'Esistenza Umana",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "enoq": "./dist/enoq_cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/cli.ts",
    "server": "ts-node src/server.ts",
    "cli": "ts-node src/cli.ts",
    "enoq": "ts-node src/enoq_cli.ts",
    "enoq:debug": "ts-node src/enoq_cli.ts --debug --field",
    "enoq:build": "ts-node src/genesis/self_builder/builder_cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:legacy": "ts-node src/test_pipeline.ts",
    "test:all:legacy": "ts-node src/test_S5_verify.ts && ts-node src/test_meta_kernel.ts && ts-node src/test_l2_execution.ts && ts-node src/test_pipeline.ts && ts-node src/test_gate_integration.ts",
    "test:s5": "ts-node src/test_S5_verify.ts",
    "test:metakernel": "ts-node src/test_meta_kernel.ts",
    "test:l2": "ts-node src/test_l2_execution.ts",
    "test:pipeline": "ts-node src/test_pipeline.ts",
    "test:gate": "ts-node src/test_gate_integration.ts",
    "test:interactive": "ts-node src/test_interactive.ts",
    "test:romantic": "ts-node src/test_romantic_vs_panic.ts",
    "test:concrescence": "ts-node src/test_concrescence.ts"
  },
  "keywords": [
    "enoq",
    "cognitive-companion",
    "ai-ethics",
    "decision-support"
  ],
  "author": "Luca Rossignoli",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "openai": "^6.15.0"
  }
}
